<Window x:Class="livelywpf.dialogues_general.Changelog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:livelywpf.dialogues_general"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="Changelog" Height="750" Width="600">
    <Grid>
        <FlowDocumentScrollViewer Name="flowDocumentViewer" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto" Foreground="Black">
            <FlowDocument Hyperlink.RequestNavigate="Hyperlink_RequestNavigate_Warning" Name="flowDocument" Background="WhiteSmoke" >
            </FlowDocument>
        </FlowDocumentScrollViewer>
    </Grid>
</Window>
