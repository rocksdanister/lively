<Window x:Class="livelywpf.WebView2Element"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:wv2="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf"
        xmlns:local="clr-namespace:livelywpf"
        Left='-9999'
        ResizeMode="NoResize"
        WindowStyle="None"
        Background="#FF2B2B2B"
        mc:Ignorable="d"
        Title="WebView2Element" Height="450" Width="800"
        Closing="Window_Closing">
    
    <DockPanel>
        <!--Source="https://www.microsoft.com"-->
        <wv2:WebView2 Name="webView" NavigationCompleted="webView_NavigationCompleted" CoreWebView2Ready="webView_CoreWebView2Ready" />
    </DockPanel>
</Window>
